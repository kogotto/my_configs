[user]
	name = Yakov Didenko
	email = kogotto@yandex-team.ru
[core]
	editor = nvim
[alias]
	hist = log --oneline --graph --decorate
	last = log -1
	staged = diff --staged
	undo = reset --soft HEAD^
	destroy = reset --hard HEAD^
	h = log --oneline --graph --decorate -10
    dd = diff develop...
    dm = diff master...
[push]
	default = simple
[submodule]
	recurse = true
